{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"slug": "gradient",
	"title": "Gradient",
	"blockTypes": [
		"designsetgo/container"
	],
	"styles": {
		"color": {
			"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-2) 0%, var(--wp--preset--color--accent-3) 100%)",
			"text": "var:preset|color|base"
		},
		"border": {
			"radius": "var:preset|spacing|30"
		},
		"spacing": {
			"padding": {
				"top": "var:preset|spacing|xl",
				"bottom": "var:preset|spacing|xl",
				"left": "var:preset|spacing|xl",
				"right": "var:preset|spacing|xl"
			}
		},
		"elements": {
			"heading": {
				"color": {
					"text": "currentColor"
				}
			},
			"h1": {
				"color": {
					"text": "currentColor"
				}
			},
			"h2": {
				"color": {
					"text": "currentColor"
				}
			},
			"h3": {
				"color": {
					"text": "currentColor"
				}
			},
			"h4": {
				"color": {
					"text": "currentColor"
				}
			},
			"h5": {
				"color": {
					"text": "currentColor"
				}
			},
			"h6": {
				"color": {
					"text": "currentColor"
				}
			}
		}
	}
}
