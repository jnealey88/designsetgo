{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "designsetgo/form-url-field",
	"version": "1.0.0",
	"title": "URL Field",
	"category": "design",
	"description": "A URL/website address input field for forms.",
	"keywords": ["form", "url", "website", "link", "address"],
	"parent": ["designsetgo/form-builder"],
	"textdomain": "designsetgo",
	"supports": {
		"html": false,
		"anchor": false,
		"customClassName": false,
		"reusable": false
	},
	"usesContext": [
		"designsetgo/form-builder/fieldSpacing",
		"designsetgo/form-builder/fieldLabelColor",
		"designsetgo/form-builder/fieldBorderColor",
		"designsetgo/form-builder/fieldBorderRadius",
		"designsetgo/form-builder/fieldFocusColor",
		"designsetgo/form-builder/inputHeight",
		"designsetgo/form-builder/inputPadding",
		"designsetgo/form-builder/fieldBackgroundColor"
	],
	"attributes": {
		"fieldName": {
			"type": "string",
			"default": ""
		},
		"label": {
			"type": "string",
			"default": "Website URL"
		},
		"placeholder": {
			"type": "string",
			"default": "https://example.com"
		},
		"helpText": {
			"type": "string",
			"default": ""
		},
		"required": {
			"type": "boolean",
			"default": false
		},
		"defaultValue": {
			"type": "string",
			"default": ""
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.scss",
	"style": "file:./style.scss"
}
